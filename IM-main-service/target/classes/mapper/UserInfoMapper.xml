<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gulincover.home.dao.UserInfoMapper">
    <select id="selectUserInfoByUserId" resultType="com.gulincover.api.entity.UserInfo">
        select * from user_info where user_id=#{userId} and is_black=0;
    </select>

</mapper>