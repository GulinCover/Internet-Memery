<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gulincover.home.dao.TopicInfoMapper">
    <select id="selectTopicInfoByTopicId" resultType="com.gulincover.api.entity.TopicInfo">
        select * from topic_info where topic_id=#{topicId} and is_black=0;
    </select>

</mapper>